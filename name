        send(id, '–î–ª—è –æ—Ñ–æ—Ä–º–ª–µ–Ω–∏—è –∑–∞—è–≤–∫–∏ - –ø–∏—à–∏—Ç–µ –Ω–∞–º: t.me/Smoky_dad_opt üòâ')
    elif msg == '–ª–∞':
        send(id, '–î–ª—è –æ—Ñ–æ—Ä–º–ª–µ–Ω–∏—è –∑–∞—è–≤–∫–∏ - –ø–∏—à–∏—Ç–µ –Ω–∞–º: t.me/Smoky_dad_opt üòâ')
    elif msg == '–¥–∞':
        send(id, '–î–ª—è –æ—Ñ–æ—Ä–º–ª–µ–Ω–∏—è –∑–∞—è–≤–∫–∏ - –ø–∏—à–∏—Ç–µ –Ω–∞–º: t.me/Smoky_dad_opt üòâ')
    elif msg == '–ù–µ—Ç':
        send(id, '–° –Ω–µ—Ç–µ—Ä–ø–µ–Ω–∏–µ–º –∂–¥—ë–º –í–∞—à–µ–≥–æ –∑–∞–∫–∞–∑–∞ üòâ')
    elif msg == '–Ω–µ—Ç':
        send(id, '–° –Ω–µ—Ç–µ—Ä–ø–µ–Ω–∏–µ–º –∂–¥—ë–º –í–∞—à–µ–≥–æ –∑–∞–∫–∞–∑–∞ üòâ')
    elif msg == '–Ω–µ—å':
        send(id, '–° –Ω–µ—Ç–µ—Ä–ø–µ–Ω–∏–µ–º –∂–¥—ë–º –í–∞—à–µ–≥–æ –∑–∞–∫–∞–∑–∞ üòâ')
    else:
        send(id, '–ú–µ–Ω—è –ø–æ–∫–∞ –Ω–µ –Ω–∞—É—á–∏–ª–∏ –æ—Ç–≤–µ—á–∞—Ç—å –Ω–∞ —Ç–∞–∫–∏–µ —Å–æ–æ–±—â–µ–Ω–∏—è :(')



bot.polling(none_stop=True)
